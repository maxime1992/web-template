{
  "name": "web-template",
  "version": "1.0.0",
  "description": "WebTemplate",
  "main": "Gruntfile.js",
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.0",
    "angular-bootstrap": "0.12.2",
    "angular-loading-bar": "0.8.0",
    "angular-mocks": "1.5.0",
    "angular-sanitize": "1.5.0",
    "angular-translate-loader-static-files": "2.8.1",
    "angular-ui-router": "1.0.0-alpha0",
    "angular-ui-router-anim-in-out": "1.0.2",
    "bootstrap": "3.3.6",
    "connect": "3.4.0",
    "font-awesome": "4.5.0",
    "rx-angular": "1.1.3",
    "rxjs": "5.0.0-beta.2",
    "opn": "4.0.0",
    "serve-static": "1.10.1",
    "xo": "0.12.1"
  },
  "xo": {
    "globals": [
      "angular",
      "app",
      "describe",
      "beforeEach",
      "inject",
      "it",
      "expect",
      "module",
      "require"
    ],
    "rules": {
      "linebreak-style": [
        0
      ],
      "new-cap": 1,
      "no-invalid-this": 0,
      "strict": [
        2,
        "function"
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": false
        }
      ]
    },
    "ignores": [
      ".tmp/**",
      ".git/**",
      "build/**"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp build&&gulp serve",
    "prod": "gulp build --production",
    "test": "gulp tests",
    "serve": "gulp serve",
    "doc": "gulp build-doc&&gulp serve-doc"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-preset-es2015": "6.3.13",
    "gulp": "git+https://git@github.com/gulpjs/gulp.git#4.0",
    "gulp-angular-embed-templates": "2.1.2",
    "gulp-babel": "6.1.1",
    "gulp-browserify": "0.5.1",
    "gulp-clean": "0.3.1",
    "gulp-concat": "2.6.0",
    "gulp-concat-css": "2.2.0",
    "gulp-connect": "2.3.1",
    "gulp-flatten": "0.2.0",
    "gulp-htmlmin": "1.3.0",
    "gulp-if": "2.0.0",
    "gulp-imagemin": "2.4.0",
    "gulp-inject": "3.0.0",
    "gulp-jshint": "2.0.0",
    "gulp-karma-runner": "1.1.8",
    "gulp-load-plugins": "1.2.0",
    "gulp-minify-css": "1.2.3",
    "gulp-ng-annotate": "1.1.0",
    "gulp-ngdocs": "0.2.13",
    "gulp-preprocess": "2.0.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-rev": "7.0.0",
    "gulp-sass": "2.2.0",
    "gulp-sass-lint": "1.1.1",
    "gulp-series": "1.0.2",
    "gulp-shell": "0.5.2",
    "gulp-size": "2.0.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-strip-comments": "2.2.1",
    "gulp-strip-debug": "1.1.0",
    "gulp-uglify": "1.5.1",
    "gulp-uncss": "1.0.4",
    "gulp-xo": "0.7.0",
    "imagemin-pngquant": "4.2.0",
    "install": "0.4.1",
    "jasmine-core": "2.4.1",
    "jshint": "2.9.1",
    "jsonfile": "2.2.3",
    "karma": "0.13.19",
    "karma-jasmine": "0.3.6",
    "karma-phantomjs-launcher": "0.2.3",
    "load-grunt-tasks": "3.4.0",
    "marked": "0.3.5",
    "merge2": "1.0.1",
    "path": "0.12.7",
    "phantomjs": "1.9.19",
    "recess": "1.1.9",
    "ghooks": "^1.0.3",
    "validate-commit-msg": "^2.0.0",
    "yargs": "4.1.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxime1992/web-template.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxime1992/web-template/issues"
  },
  "homepage": "https://github.com/maxime1992/web-template#readme"
}
