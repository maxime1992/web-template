{
  "name": "web-template",
  "version": "2.1.0",
  "description": "WebTemplate",
  "main": "Gulpfile.js",
  "engines": {
    "node": ">= 5.0.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.0",
    "angular-loading-bar": "0.8.0",
    "angular-mocks": "1.5.0",
    "angular-sanitize": "1.5.0",
    "angular-translate": "2.9.2",
    "angular-translate-loader-static-files": "2.9.2",
    "angular-ui-bootstrap": "1.2.0",
    "angular-ui-router": "1.0.0-beta.1",
    "angular-ui-router-anim-in-out": "1.0.2",
    "bootstrap": "3.3.6",
    "font-awesome": "4.5.0",
    "ng-webworker": "github:mattslocum/ng-webworker",
    "rx": "4.0.8",
    "rx-angular": "1.1.3"
  },
  "xo": {
    "globals": [
      "angular",
      "app",
      "describe",
      "beforeEach",
      "inject",
      "it",
      "expect",
      "module",
      "require"
    ],
    "rules": {
      "linebreak-style": [
        0
      ],
      "new-cap": 1,
      "no-invalid-this": 0,
      "strict": [
        2,
        "function"
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": false
        }
      ]
    },
    "ignores": [
      ".tmp/**",
      ".git/**",
      "build/**"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp build&&gulp serve",
    "prod": "gulp build --production",
    "test": "gulp unit&&gulp e2e",
    "serve": "gulp serve",
    "doc": "gulp build-doc&&gulp serve-doc --PORT=8181"
  },
  "devDependencies": {
    "babel-core": "6.5.2",
    "babel-preset-es2015": "6.5.0",
    "babel-runtime": "6.5.0",
    "connect": "3.4.1",
    "conventional-github-releaser": "1.1.0",
    "critical": "0.7.0",
    "del": "2.2.0",
    "ghooks": "1.0.3",
    "git-semver-tags": "1.1.2",
    "gulp": "https://git@github.com/gulpjs/gulp.git#4.0",
    "gulp-angular-embed-templates": "2.2.0",
    "gulp-autoprefixer": "3.1.0",
    "gulp-babel": "6.1.2",
    "gulp-browserify": "0.5.1",
    "gulp-bump": "1.0.0",
    "gulp-complexity": "0.3.2",
    "gulp-concat": "2.6.0",
    "gulp-concat-css": "2.2.0",
    "gulp-connect": "2.3.1",
    "gulp-conventional-changelog": "1.1.0",
    "gulp-cssnano": "2.1.1",
    "gulp-decomment": "0.1.3",
    "gulp-flatten": "0.2.0",
    "gulp-gh-pages": "0.5.4",
    "gulp-git": "1.7.0",
    "gulp-gzip": "1.2.0",
    "gulp-htmlmin": "1.3.0",
    "gulp-if": "2.0.0",
    "gulp-imagemin": "2.4.0",
    "gulp-inject": "3.0.0",
    "gulp-karma-runner": "1.1.9",
    "gulp-load-plugins": "1.2.0",
    "gulp-ng-annotate": "2.0.0",
    "gulp-ngdocs": "0.2.13",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-preprocess": "2.0.0",
    "gulp-protractor": "2.1.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-rev": "7.0.0",
    "gulp-sass": "2.2.0",
    "gulp-sass-lint": "1.1.1",
    "gulp-series": "1.0.2",
    "gulp-size": "2.0.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-strip-debug": "1.1.0",
    "gulp-uglify": "1.5.3",
    "gulp-uncss": "1.0.4",
    "gulp-util": "3.0.7",
    "gulp-xo": "0.8.0",
    "gulp-zip": "3.2.0",
    "imagemin-pngquant": "4.2.2",
    "jasmine": "2.4.1",
    "jasmine-core": "2.4.1",
    "jpegtran": "0.2.0",
    "jpegtran-bin": "3.0.6",
    "jsonfile": "2.2.3",
    "karma": "0.13.9",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "karma-phantomjs-launcher": "1.0.0",
    "less": "2.6.0",
    "marked": "0.3.5",
    "merge2": "1.0.1",
    "node-notifier": "4.5.0",
    "node-sass": "3.4.2",
    "opn": "4.0.0",
    "path": "0.12.7",
    "plato": "1.5.0",
    "recess": "1.1.9",
    "reflect-metadata": "0.1.3",
    "serve-static": "1.10.2",
    "validate-commit-msg": "2.1.0",
    "xo": "0.12.1",
    "yargs": "4.2.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxime1992/web-template.git"
  },
  "author": "Maxime Robert <maxime.robert1992@gmail.com>",
  "contributors": [
    "Kevin CARADANT <kevin.caradant@gmail.com>",
    "Kazhord <perso@kazhord.fr>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxime1992/web-template/issues"
  },
  "homepage": "https://github.com/maxime1992/web-template#readme"
}
